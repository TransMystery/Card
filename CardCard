$('button').click(function() {
    $('h2').text(randomEl(adjectives)+' '+randomEl(nouns));
    selectElementContents($('h2')[0]);
});

function randomEl(list) {
    var i = Math.floor(Math.random() * list.length);
    return list[i];
}

function selectElementContents(el) {
    var range = document.createRange();
    range.selectNodeContents(el);
    var sel = window.getSelection();
    sel.removeAllRanges();
    sel.addRange(range);
}

var adjectives = ["Do you think I've ever been in love?", "What about me is new or unfamiliar?", "Do you think I've ever had my heart broken?", "What do you think I'd splurge a lot of money on?", "What do you think I'm going to do in the future (work/employment)?", "What was your first impression of me?", "Do you think I was popular in high school?", "On a scale of 1 - 10, how messy do you think my car is?", "Do you think I like to read?", "Describe what type of music my favorite musician would make?(Take a guess)", "Where do you think I grew up? What was the environment?"];
var nouns = ["❤️", "💙", "💛", "💚","🧡","💗","💕","💝"];

$('button').click();
